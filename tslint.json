{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-eslint-rules"
  ],
  "rules": {
    "only-arrow-functions": false,
    "object-literal-sort-keys": false,
    "prettier": [true, { 
      "singleQuote": true,
      "semi": false
    }]
  }
}
